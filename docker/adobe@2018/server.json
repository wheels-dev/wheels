{
	"profile": "development",
	"force":true,
	"web":{
			"host":"localhost",
			"webroot":"public",
			"http":{
					"port":"62018",
					"enable":true
			},
			"rewrites":{
					"enable":true,
					"config":"public/urlrewrite.xml"
			}
	},
	"app":{
			"cfengine":"adobe@2018.0.10+320417",
			"libDirs":"./docker/adobe@2018/lib",
			"serverHomeDirectory":".engine/adobe@2018"
	},
	"cfconfigFile":"CFConfig.json",
	"JVM":{
		"args":"-Djava.class.path=./docker/adobe@2018/lib/ojdbc11.jar"
	},
	"scripts":{
        "onServerInstall":"cfpm install image,mail,zip,caching,sqlserver"
    }
}
